{
  "EngineConfiguration": {
    "DefaultEngineType": "HelloblueGK",
    "DefaultEfficiency": 0.92,
    "MaxSimulationTime": 900,
    "UseAdvancedSolvers": true,
    "OpenFOAMPath": "/opt/openfoam8",
    "EnableRealTimeTelemetry": true,
    "EnableQuantumAdvantage": false,
    "EnableShapeShifting": false
  },
  "ApiUrl": "https://hellobluegk-demo-production.up.railway.app",
  "ApiTimeout": 30000,
  "MaxConcurrentRequests": 200,
  "EnablePerformanceMonitoring": true,
  "MetricsInterval": 5000,
  "EnableRateLimiting": true,
  "DefaultRateLimit": 500,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "File": {
      "Path": "logs/hellobluegk.log",
      "RollingInterval": "Day"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "change-me-in-production",
    "Issuer": "hellobluegk-demo"
  },
  "Cors": {
    "Origins": "https://hellobluegk-demo-production.up.railway.app"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=HelloblueGK;Trusted_Connection=true;MultipleActiveResultSets=true",
    "PostgreSQLConnection": "Host=localhost;Database=hellobluegk;Set_Username_and_Password_via_ConnectionStrings__PostgreSQLConnection_env_var",
    "RedisConnection": "localhost:6379",
    "RabbitMQConnection": "amqp://Set_user_and_password_via_ConnectionStrings__RabbitMQConnection_env_var"
  },
  "AerospaceEngine": {
    "SimulationSettings": {
      "CFD": {
        "GridResolution": 1000000,
        "TurbulenceModels": ["k-epsilon", "k-omega", "LES"],
        "ParallelCores": 32,
        "ConvergenceTolerance": 1e-6,
        "MaxIterations": 10000
      },
      "Thermal": {
        "MeshElements": 1000000,
        "HeatTransferModes": ["Conduction", "Convection", "Radiation"],
        "MaterialDatabase": "AerospaceMaterials.json",
        "ConvergenceTolerance": 1e-5
      },
      "Structural": {
        "MeshElements": 500000,
        "AnalysisTypes": ["Static", "Dynamic", "Fatigue", "Buckling"],
        "MaterialModels": ["Linear", "Nonlinear", "Plasticity"],
        "SafetyFactor": 1.5
      },
      "Electromagnetic": {
        "FieldResolution": 100000,
        "MaxwellSolver": "FEM",
        "FrequencyRange": [0, 1000000]
      },
      "MolecularDynamics": {
        "Atoms": 1000000,
        "TimeStep": 1e-15,
        "Temperature": 300,
        "Ensemble": "NVT"
      }
    },
    "AI": {
      "MachineLearning": {
        "ModelPath": "Models/",
        "TrainingDataPath": "Data/Training/",
        "ValidationDataPath": "Data/Validation/",
        "PredictionAccuracy": 0.999
      },
      "Optimization": {
        "GeneticAlgorithm": {
          "PopulationSize": 100,
          "Generations": 1000,
          "MutationRate": 0.01,
          "CrossoverRate": 0.8
        },
        "NeuralNetwork": {
          "Layers": [64, 128, 256, 128, 64],
          "ActivationFunction": "ReLU",
          "LearningRate": 0.001,
          "BatchSize": 32
        }
      }
    },
    "DigitalTwin": {
      "RealTimeLearning": true,
      "PredictionAccuracy": 0.999,
      "ModelUpdateFrequency": "1h",
      "DataRetentionDays": 365
    },
    "Enterprise": {
      "Authentication": {
        "JWTSecret": "REPLACE_WITH_ENV_VAR_OR_OVERRIDE_VIA_AerospaceEngine__Enterprise__Authentication__JWTSecret",
        "TokenExpirationHours": 24,
        "RequireHttps": true
      },
      "Security": {
        "EncryptionAlgorithm": "AES-256",
        "KeyRotationDays": 30,
        "AuditLogging": true
      },
      "Monitoring": {
        "ApplicationInsights": {
          "InstrumentationKey": "REPLACE_WITH_ENV_VAR_OR_OVERRIDE_VIA_AerospaceEngine__Enterprise__Monitoring__ApplicationInsights__InstrumentationKey",
          "EnablePerformanceCounters": true,
          "EnableDependencyTracking": true
        },
        "HealthChecks": {
          "Database": true,
          "ExternalServices": true,
          "MemoryUsage": true,
          "CPUUsage": true
        }
      }
    }
  },
  "CloudServices": {
    "AWS": {
      "Region": "us-east-1",
      "S3Bucket": "hellobluegk-simulation-data",
      "CloudWatch": {
        "LogGroup": "HelloblueGK",
        "MetricsNamespace": "AerospaceEngine"
      }
    },
    "Azure": {
      "StorageAccount": "hellobluegkstorage",
      "ApplicationInsights": {
        "ConnectionString": "REPLACE_WITH_ENV_VAR_OR_OVERRIDE_VIA_CloudServices__Azure__ApplicationInsights__ConnectionString"
      }
    },
    "GoogleCloud": {
      "ProjectId": "hellobluegk-project",
      "StorageBucket": "hellobluegk-simulation-data"
    }
  },
  "ExternalPhysicsSolvers": {
    "OpenFOAM": {
      "InstallationPath": "/opt/openfoam",
      "Version": "v2212",
      "ParallelExecution": true,
      "MaxCores": 64
    },
    "ANSYS": {
      "InstallationPath": "C:\\Program Files\\ANSYS Inc\\v221",
      "LicenseServer": "localhost",
      "MaxLicenses": 10
    },
    "Abaqus": {
      "InstallationPath": "/opt/abaqus",
      "Version": "2022",
      "LicenseServer": "localhost"
    },
    "COMSOL": {
      "InstallationPath": "/opt/comsol",
      "Version": "6.0",
      "LicenseServer": "localhost"
    }
  },
  "CADIntegration": {
    "SupportedFormats": ["STEP", "IGES", "STL", "OBJ", "PLY"],
    "AutoCAD": {
      "Enabled": true,
      "Version": "2024"
    },
    "SolidWorks": {
      "Enabled": true,
      "Version": "2024"
    },
    "CATIA": {
      "Enabled": true,
      "Version": "V6"
    },
    "NX": {
      "Enabled": true,
      "Version": "2206"
    }
  },
  "Manufacturing": {
    "3DPrinting": {
      "SupportedTechnologies": ["SLS", "SLM", "EBM", "DMLS"],
      "MaterialDatabase": "ManufacturingMaterials.json",
      "QualityControl": true
    },
    "CNCMachining": {
      "SupportedOperations": ["Milling", "Turning", "Drilling"],
      "ToolDatabase": "CNCTools.json",
      "PostProcessing": true
    }
  },
  "Validation": {
    "RealWorldData": {
      "RaptorEngine": "Data/Validation/Raptor/",
      "MerlinEngine": "Data/Validation/Merlin/",
      "RS25Engine": "Data/Validation/RS25/",
      "TestStandData": "Data/Validation/TestStands/"
    },
    "AccuracyThresholds": {
      "Thrust": 0.95,
      "SpecificImpulse": 0.90,
      "ChamberPressure": 0.92,
      "ThermalPerformance": 0.88,
      "StructuralIntegrity": 0.95
    }
  },
  "Performance": {
    "SimulationSpeed": {
      "TargetCalculationsPerSecond": 1000000,
      "ParallelEfficiency": 0.95,
      "MemoryOptimization": true
    },
    "Scalability": {
      "MaxConcurrentSimulations": 100,
      "LoadBalancing": true,
      "AutoScaling": true
    }
  }
} 